! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #262626, #d5c4a1, #262626
rofi.color-normal: #262626, #d5c4a1, #262626, #928374, #ebdbb2
rofi.color-active: #262626, #d5c4a1, #262626, #fabd2f, #ebdbb2
rofi.color-urgent: #262626, #d5c4a1, #262626, #fb4934, #ebdbb2
rofi.bw: 2
rofi.padding: 25

Xft.dpi:                  96
Xft.antialias:            true
Xft.autohint:             false
Xft.hinting:              true
Xft.hintstyle:            hintnone
Xft.lcdfilter:            lcdlight
Xft.rgba:                 rgb

URxvt*font:               xft:Source Code Pro:pixelsize=14
URxvt*lineSpace:          1
URxvt.letterSpace:        0
URxvt.iso14755:           false
URxvt.iso14755_52:        false
URxvt*geometry:           80x24
URxvt.scrollBar:          false
URxvt*loginShell:         true
URxvt.internalBorder:     40
URxvt.perl-ext-common:    default,clipboard,url-select,keyboard-select,font-size
URxvt.clipboard.autocopy: true
URxvt*buffered:           true

URxvt.keysym.C-Up:        font-size:increase
URxvt.keysym.C-Down:      font-size:decrease
URxvt.keysym.C-S-Up:      font-size:incglobal
URxvt.keysym.C-S-Down:    font-size:decglobal
URxvt.keysym.C-equal:     font-size:reset
URxvt.keysym.C-slash:     font-size:show

! colors
*.cursorColor:  #ebdbb2

! hard contrast: *background: #1d2021
*.background: #262626

! soft contrast: *background: #32302f
*.foreground: #ebdbb2

! Black + DarkGrey
